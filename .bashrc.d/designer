desdir=/opt/biznuvo/apps/development#design#operations
deslog=/opt/biznuvo/logs/development#design#operations

alias cdd='cd $desdir'
alias cdl='cd $deslog'
alias cdw='cd $desdir/work'

alias tfd='tail -F $deslog/app.log'
alias lld='less $deslog/app.log'

alias cpapp='cp $desdir/work/app-development#design#operations.tgz /mnt/shared'
alias updatedes='sudo -u tomcat /opt/tomcat/updatedes.sh'
